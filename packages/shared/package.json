{
  "name": "@demonicka/shared",
  "version": "1.0.0",
  "description": "Shared authentication and authorization logic for Démonická",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
